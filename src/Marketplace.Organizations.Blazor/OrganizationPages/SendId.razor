@page "/SendId"
@inject NavigationManager Manager
<h3>SendId</h3>
<form class="row g-3" style="width: 400px ;margin: auto" >
        <div>
            <input @bind="Id"class="form-control" type="text" placeholder="Enter OrganizationId"/>
        </div>
        <button class="form-control" type="button" @onclick="Send">Find</button>
    </form>
@code {
    private Guid Id { get; set; }
    private async Task Send()
    {
        Manager.NavigateTo($"GetById/{Id}");
    }

}